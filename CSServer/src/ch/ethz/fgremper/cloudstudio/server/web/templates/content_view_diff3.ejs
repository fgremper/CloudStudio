<% if (content.length == 0) { %>

	<div class="emptyView">All files are empty.</div>

<% } else { %>

	<div id="contentDiffHeader">
		<div><% if (viewAsOrigin) { %>Origin<% } else { %>Me<% } %></div>
		<div>Common Ancestor</div>
		<div><span class="userSmall"><%= theirUsername %></span></div>
	</div>

	<table id="contentDiff">
		<tr>
			<td class="diffNumberColumn">
				<% var myCount = 1; for (i = 0; i < content.length; i++) { %>
					<% if (content[i].myType != 'PAD' && content[i].myType != 'MODIFIED_PAD' && content[i].myType != 'CONFLICT_PAD') { %><div class="diffNumber"><%= myCount %></div><% myCount++ %>
					<% } else { %><div class="diffNumber">&nbsp;</div><% } %>
				<% } %>
			</td>
			<td>
				<div class="diffBox">
				<% for (i = 0; i < content.length; i++) { %>
					<% if (content[i].myType == 'CONFLICT' || content[i].myType == 'CONFLICT_PAD') { %><div class="diffLine diffConflict">
					<% } else if (content[i].myType == 'PAD') { %><div class="diffLine diffPad">
					<% } else if (content[i].myType == 'MODIFIED' || content[i].myType == 'MODIFIED_PAD') { %><div class="diffLine diffModify">
					<% } else { %><div class="diffLine"><% } %>
						<span class="pre"><%= content[i].myContent %></span>&nbsp;
					</div>
				<% } %>
				</div>
			</td>
			<td class="diffNumberColumn">
				<% var myCount = 1; for (i = 0; i < content.length; i++) { %>
					<% if (content[i].baseType != 'PAD' && content[i].baseType != 'MODIFIED_PAD' && content[i].baseType != 'CONFLICT_PAD') { %><div class="diffNumber"><%= myCount %></div><% myCount++ %>
					<% } else { %><div class="diffNumber">&nbsp;</div><% } %>
				<% } %>
			</td>
			<td>
				<div class="diffBox">
				<% for (i = 0; i < content.length; i++) { %>
					<% if (content[i].baseType == 'CONFLICT' || content[i].baseType == 'CONFLICT_PAD') { %><div class="diffLine diffConflict">
					<% } else if (content[i].baseType == 'PAD') { %><div class="diffLine diffPad">
					<% } else if (content[i].baseType == 'MODIFIED' || content[i].baseType == 'MODIFIED_PAD') { %><div class="diffLine diffModify">
					<% } else { %><div class="diffLine"><% } %>
						<span class="pre"><%= content[i].baseContent %></span>&nbsp;
					</div>
				<% } %>
				</div>
			</td>
			<td class="diffNumberColumn">
				<% var myCount = 1; for (i = 0; i < content.length; i++) { %>
					<% if (content[i].theirType != 'PAD' && content[i].theirType != 'MODIFIED_PAD' && content[i].theirType != 'CONFLICT_PAD') { %><div class="diffNumber"><%= myCount %></div><% myCount++ %>
					<% } else { %><div class="diffNumber">&nbsp;</div><% } %>
				<% } %>
			</td>
			<td>
				<div class="diffBox">
				<% for (i = 0; i < content.length; i++) { %>
					<% if (content[i].theirType == 'CONFLICT' || content[i].theirType == 'CONFLICT_PAD') { %><div class="diffLine diffConflict">
					<% } else if (content[i].theirType == 'PAD') { %><div class="diffLine diffPad">
					<% } else if (content[i].theirType == 'MODIFIED' || content[i].theirType == 'MODIFIED_PAD') { %><div class="diffLine diffModify">
					<% } else { %><div class="diffLine"><% } %>
						<span class="pre"><%= content[i].theirContent %></span>&nbsp;
					</div>
				<% } %>
				</div>
			</td>
		</tr>
	</table>

<% } %>

