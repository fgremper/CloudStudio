<div id="header">
	<div id="headerText">
		RTCA Web Interface
	</div>
</div>
<div id="title">
    <div id="titleText">
    	<span class="repositoryViewButton clickable">Repositories</span> / <span class="branchViewButton clickable"><%= repositoryAlias %></span> / <strong><%= branch %></strong>
    </div>
</div>
<div id="view">
	View uncommitted:
	<input type="checkbox" id="filterViewUncommitted">
	<div id="submitFilter"><strong>Go!</strong></div>

	<div id="conflicts">

	</div>


	<table>
	    <tr class="descriptors"><th>Filename</th><th>Users</th></tr>
	    <tr class="heading"><td colspan="2">Branches (<%= branches.length %>)</td></tr>
		<% for (var i = 0; i < branches.length; i++) { %>
		    <tr class="branch clickable" data-branch="<%= branches[i]['branch'] %>">
		        <td><span class="blue"><%= branches[i]['branch'] %></span></td>
		        <td>
		 			<% for (var j = 0; j < branches[i].users.length; j++) { %>
		 				<span class="user"><%= branches[i].users[j].username %></span> <%= branches[i].users[j].relationWithOrigin %><br>
		 			<% } %>
				</td>
		    </tr>
		<% } %>
	</table>

</div>
